       JSR_ABS    Byte = '20'H,   -- Jump to subroutine
    
       LDA_IMM Byte = 'A9'H,   -- Load Accumulator
       LDA_ZP     Byte = 'A5'H,
       LDA_ZPX  Byte = 'B5'H,
       LDA_ABS  Byte = 'AD'H,
       LDA_ABSX Byte = 'BD'H,
       LDA_ABSY Byte = 'B9'H,
       LDA_INDX Byte = 'A1'H,
