       AND_ZP Byte = '25'H,
       AND_ZPX Byte = '35'H,
       AND_ABS Byte = '2D'H,
       AND_ABSX Byte = '3D'H,
       AND_ABSY Byte = '39'H,
       AND_INDX Byte = '21'H,
       AND_INDY Byte = '31'H,
    
       ASL_ACC Byte = '0A'H,  -- Arithmetic Shift Left
