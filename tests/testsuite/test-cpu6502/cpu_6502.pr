    /* CIF procedure (5927, 156), (88, 35) */
    procedure Store_Byte;
        /* CIF TEXT (87, 61), (170, 140) */
        fpar 
           in index Word, 
           in val Byte;
        /* CIF ENDTEXT */
        /* CIF START (397, 93), (70, 35) */
        START;
            /* CIF PROCEDURECALL (354, 148), (155, 35) */
            call Write_Byte (index, val);
            /* CIF task (366, 203), (130, 35) */
